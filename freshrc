# freshshell.com
#
# Examples:
#
#   fresh twe4ked/dotfiles shell/functions.sh
#   fresh jasoncodes/dotfiles aliases/rails.sh
#   fresh jasoncodes/dotfiles config/tmux.conf --file
#
# See http://freshshell.com/readme for documentation.

fresh freshshell/fresh bin/fresh --bin

fresh config/tmux.conf --file=~/.tmux.conf
fresh config/pythonrc --file=~/.pythonrc
fresh config/powerline --file=~/.config/powerline/
# TODO: Enable this once the config is dialed in a bit more
# fresh config/sway/config --file=~/.config/sway/config

fresh shell/aliases/\*
fresh shell/\*

fresh ghing/vim-config . --file=~/.vim/
fresh ghing/vim-config vimrc --file=~/.vimrc
